<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>


<BorderPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.admin.RegNumMaintenanceController">
   <center>
      <TableView fx:id="vehicleTable" prefHeight="249.0" prefWidth="607.0">
          <columns>
              <TableColumn fx:id="vehicleState" minWidth="6.0" prefWidth="124.5" text="Registration number" />
              <TableColumn fx:id="carState" minWidth="0.0" prefWidth="95.0" text="Vehicle state" />
              <TableColumn fx:id="buttonsCol" prefWidth="137.5" />
          </columns>
      </TableView>
   </center>
   <top>
      <Pane prefHeight="51.0" prefWidth="607.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="regNumTextField" layoutX="8.0" layoutY="14.0" promptText="A007AA" />
            <Button layoutX="326.0" layoutY="14.0" mnemonicParsing="false" text="Button" />
            <ComboBox fx:id="stateBox" layoutX="167.0" layoutY="13.0" prefWidth="150.0" promptText="On maintenance" />
         </children>
      </Pane>
   </top>
</BorderPane>
